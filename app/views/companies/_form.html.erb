<% if @company.errors.any? %>
<h2>The following errors prevented the company from being saved</h2>
<ul>
  <% @company.errors.full_messages.each do |msg| %>
  <li><%= msg %></li>
  <% end %>
</ul>
<% end %>
<%= form_with(model: @company,local: true) do |f| %>
<p>
  <%= f.label :name %><br/>
  <%= f.text_field :name %>
</p>
<p>
  <%= f.label :place %><br/>
  <%= f.text_field :place %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>
