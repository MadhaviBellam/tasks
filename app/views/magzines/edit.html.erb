<h1>Edit Magzine</h1>
<% if @magzine.errors.any? %>
<h2>The following errors prevented the magzine from being saved</h2>
  <ul>
    <% @magzine.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end %>
  </ul>
<% end %>
<%= form_with(model: @magzine,local: true) do |f| %>
<p>
  <%= f.label :title %><br/>
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :description %><br/>
  <%= f.text_area :description %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>
